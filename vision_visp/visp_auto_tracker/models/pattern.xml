<?xml version="1.0"?>
<conf>
	<ecm>
		<mask>
			<size>5</size>
			<nb_mask>180</nb_mask>
		</mask>
		<range>
			<tracking>10</tracking>
		</range>
		<contrast>
			<edge_threshold>5000</edge_threshold>
			<mu1>0.5</mu1>
			<mu2>0.5</mu2>
		</contrast>
	</ecm>
	<sample>
		<step>4</step>
		<nb_sample>250</nb_sample>
	</sample>
	<klt>
		<threshold_outlier>0.7</threshold_outlier> 
  		<max_features>10000</max_features> 
  		<window_size>5</window_size> 
  		<quality>0.05</quality> 
  		<min_distance>20</min_distance> 
  		<harris>0.01</harris>
		<size_block>3</size_block> 
  		<pyramid_lvl>3</pyramid_lvl> 
		<mask_border>0</mask_border>
	</klt>
	<face>
  		<angle_appear>75</angle_appear> 
  		<angle_disappear>75</angle_disappear>
	</face>
</conf>

